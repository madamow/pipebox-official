master_use_fwthreads = True

# these 2 are for debugging purposes
purge_job_dir = never
compress_cleanup = False
det_bands = {{args.det_bands}}

basket = {{args.tile}} # FIX THIS
proctag = {{args.proctag}}
depth= deep
mosaictype = UNCHANGED

tilename = {{args.tile}}
da_tilename = {{args.tile}}
unitname = ${tilename}

bands = {{ args.bands }}

{% if args.rundir %}
ops_run_dir = {{ args.rundir }}
{% else %}
ops_run_dir = ${project}/${pipeline}/${campaign}/r${reqnum}/${unitname}/p${attnum:2}
{% endif %}

nthreads = {{ args.nthreads }}

save_run_vals = tilename, jira_id

<<inclfunc mepipelineappintg.mepochmisc.get_tile_info(submit_des_services, submit_des_db_section, tilename)>>

request_cpus = {{args.request_cpus}}
######################################################################
blocklist = me #,meds
<block>
    <me>
     modulelist = combine-se-cat, coadd-astrorefine, split-scamp-head, bigquery, null-weight-for-coadd, band-swarp-wgt, band-swarp-msk, band-swarp-wgt-nobkg,band-swarp-msk-nobkg, band-assemble, band-assemble-nobkg, det-swarp-wgt, det-swarp-msk, det-assemble, make-color-img, make-ptif, band-create-psfex-catalog, det-create-psfex-catalog, band-psfex, det-psfex, band-coadd-catalog, det-catalog #, extinction, healpix, wavg, mepoch-ingest-no-mangle
    #modulelist = combine-se-cat, coadd-astrorefine, split-scamp-head, bigquery, null-weight-for-coadd, band-swarp-wgt, band-swarp-msk, band-swarp-wgt-nobkg,band-swarp-msk-nobkg, band-assemble, band-assemble-nobkg, det-swarp-wgt, det-swarp-msk, det-assemble, make-color-img, make-ptif, make-color-img-nobkg,make-ptif-nobkg,band-create-psfex-catalog, det-create-psfex-catalog, band-psfex, det-psfex, band-coadd-catalog, det-catalog #, extinction, healpix, wavg, mangle,mepoch-ingest
    </me>
    <meds>
        modulelist = medsquery,run-desmeds
    </meds>
</block>


{% endblock %}


